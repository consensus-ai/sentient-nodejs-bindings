{
  "name": "sentient.js",
  "version": "0.0.1",
  "description": "Node wrapper for the sentient-network API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consensus-ai/sentient-nodejs-bindings.git"
  },
  "main": "lib/sentient.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "test": "npm run lint && mocha --compilers js:babel-register --recursive ./test",
    "prepare": "npm run build",
    "lint": "eslint --max-warnings 0 ./src ./test",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "sentient-network",
    "sentient-ui",
    "Decentralized",
    "Altcoin",
    "Cryptocurrency"
  ],
  "bugs": {
    "url": "https://github.com/consensus-ai/sentient-nodejs-bindings/issues"
  },
  "author": "Consensus AI",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^4.1.2",
    "eslint": "4.18.2",
    "mocha": "^5.2.0",
    "nock": "^9.0.18",
    "proxyquire": "^1.7.10",
    "readdir": "0.0.13",
    "sinon": "^7.2.2"
  },
  "dependencies": {
    "@sinonjs/commons": "^1.3.0",
    "@sinonjs/formatio": "^3.1.0",
    "@sinonjs/samsam": "^3.0.2",
    "bignumber.js": "^4.0.4",
    "request": "^2.88.0"
  }
}
